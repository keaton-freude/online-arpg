{
  "name": "babylonjs-starter",
  "version": "1.0.0",
  "description": "BabylonJS starter",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "nodemon --exec ./node_modules/.bin/ts-node -- ./bin/server.ts",
    "start:prod": "set NODE_ENV=production ts-node ./bin/server.ts",
    "build":
      "npm run clean && webpack --config src/config/webpack.prod.ts --progress --profile --bail"
  },
  "author": "Remy Korrelboom",
  "license": "ISC",
  "dependencies": {
    "babylonjs": "~3.2.0-beta.4",
    "debug": "~3.1.0",
    "express": "~4.16.3",
    "ts-loader": "~4.1.0",
    "ts-node": "~5.0.1",
    "typescript": "~2.8.1",
    "webpack": "~4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-middleware": "~3.1.2",
    "webpack-merge": "~4.1.0",
    "nodemon": "~1.17.3"
  },
  "devDependencies": {
    "@types/debug": "0.0.30",
    "@types/express": "~4.11.1",
    "@types/node": "~9.6.2",
    "@types/webpack": "~4.1.3",
    "copy-webpack-plugin": "~4.5.1",
    "css-loader": "~0.28.1",
    "file-loader": "~1.1.11",
    "html-loader": "~0.5.5",
    "html-webpack-plugin": "~3.2.0",
    "null-loader": "~0.1.1",
    "rimraf": "~2.6.1",
    "style-loader": "~0.20.3"
  }
}
